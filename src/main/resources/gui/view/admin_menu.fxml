<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox spacing="10" alignment="CENTER" xmlns:fx="http://javafx.com/fxml" fx:controller="gui.controller.AdminMenuController">
    <Label text="🛠️ Admin-Menü" />
    <Button text="Statistiken zurücksetzen" onAction="#onResetStats" />
    <Button text="Benutzer anzeigen" onAction="#onShowUsers" />
    <Button text="Benutzer löschen" onAction="#onDeleteUser" />
    <Button text="Neuen Benutzer registrieren" onAction="#onRegisterUser" />
    <Button text="Zurück zum Hauptmenü" onAction="#onBack" />
    <Label fx:id="outputLabel" wrapText="true" />
</VBox>